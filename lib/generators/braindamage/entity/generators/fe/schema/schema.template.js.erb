export default {
  modelName: '<%= singular_class_name %>',
  attributes: {
    <%- exposed_attributes.values.sort_by(&:fe_name).each do |attribute| -%>
      <%= attribute.fe_name %>: <%= pretty_print_thing(attribute, 2) %>,
    <%- end -%>
  },
  names: {
    <%- name_map.each do |name_method, value| -%>
    <%= name_method %>: "<%= value -%>",
    <%- end -%>
  },
  validators: <%= pretty_print_thing(printable_validators, 1) %>,
  enums: <%= pretty_print_thing(exposed_enums, 1) %>,
}
